{
  "name": "duolibras-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/src/index.js",
    "dev": "tsx watch src/index.ts --ignore prisma/seed.ts",
    "typecheck": "tsc --noEmit",
    "test": "jest --watchAll --verbose",
    "db:push": "pnpm prisma db push",
    "db:migrate:dev": "pnpm prisma migrate dev",
    "db:up": "docker-compose up -d",
    "db:stop": "docker-compose stop",
    "db:down": "docker-compose down",
    "db:seed": "pnpm prisma db seed",
    "build": "pnpm build:clear && pnpm build:generate",
    "build:generate": "swc src -d build",
    "build:clear": "rm -rf build/"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "cors": "^2.8.5",
    "eslint": "^8.54.0",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.731.1",
    "@aws-sdk/lib-storage": "^3.731.1",
    "@aws-sdk/s3-request-presigner": "^3.731.1",
    "@prisma/client": "6.2.1",
    "@types/multer": "^1.4.12",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fs-extra": "^11.3.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "multer": "1.4.5-lts.1",
    "ulid": "^2.3.0",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  }
}
